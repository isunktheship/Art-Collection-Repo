= form_for @print_size do |f|
  - if @print_size.errors.any?
    #error_explanation
      %h2= "#{pluralize(@print_size.errors.count, "error")} prohibited this print_size from being saved:"
      %ul
        - @print_size.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :height
    = f.number_field :height
  .field
    = f.label :width
    = f.number_field :width
  .actions
    = f.submit 'Save'
